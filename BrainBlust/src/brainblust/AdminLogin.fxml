<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="brainblust.AdminLoginController">
   <children>
      <Label layoutX="166.0" layoutY="38.0" text="Admin Login">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="58.0" layoutY="94.0" text="Admin User id:" />
      <Label layoutX="44.0" layoutY="143.0" text="Admin Password:" />
      <TextField fx:id="adminid" layoutX="166.0" layoutY="90.0" promptText="admin user id" />
      <TextField fx:id="adminpass" layoutX="166.0" layoutY="139.0" promptText="admin password" />
      <Button fx:id="adminsubmit" layoutX="194.0" layoutY="200.0" mnemonicParsing="false" onAction="#submit" prefHeight="0.0" prefWidth="78.0" text="Login">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Label fx:id="msglabeladmin" alignment="CENTER" contentDisplay="CENTER" layoutX="134.0" layoutY="300.0" prefHeight="62.0" prefWidth="183.0" text="Label" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Hyperlink fx:id="studentlogin" layoutX="182.0" layoutY="251.0" onAction="#loginstudent" text="Student Login">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>

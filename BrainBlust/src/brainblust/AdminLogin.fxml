<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="brainblust.AdminLoginController">
   <children>
      <Label layoutX="112.0" layoutY="40.0" text="Admin Login">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="24.0" layoutY="94.0" text="Admin User id:" />
      <Label layoutX="11.0" layoutY="143.0" text="Admin Password:" />
      <TextField fx:id="adminid" layoutX="119.0" layoutY="90.0" promptText="admin user id" />
      <TextField fx:id="adminpass" layoutX="119.0" layoutY="139.0" promptText="admin password" />
      <Button fx:id="adminsubmit" layoutX="133.0" layoutY="191.0" mnemonicParsing="false" onAction="#submit" prefHeight="0.0" prefWidth="78.0" text="Login">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Label fx:id="msglabeladmin" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="302.0" prefHeight="133.0" prefWidth="316.0" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Hyperlink fx:id="studentlogin" layoutX="121.0" layoutY="245.0" onAction="#loginstudent" text="Student Login">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
